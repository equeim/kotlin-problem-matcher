{
    "problemMatcher": [
        {
            "owner": "kotlin-problem-matcher-file-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^e: file:\\/\\/(.*):(\\d+):(\\d+) (.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        },
        {
            "owner": "kotlin-problem-matcher-file-warning",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^w: file:\\/\\/(.*):(\\d+):(\\d+) (.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        },
        {
            "owner": "kotlin-problem-matcher-global-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^e: (.*)$",
                    "message": 1
                }
            ]
        },
        {
            "owner": "kotlin-problem-matcher-global-warning",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^w: (.*)$",
                    "message": 1
                }
            ]
        }
    ]
}
